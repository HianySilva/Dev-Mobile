        <activity android:name=".FormSymptomatologicActivity" android:exported="false" />
        <activity android:name=".GeralActivity" android:exported="false" />
        <activity android:name=".CardiovascularActivity" android:exported="false" />
        <activity android:name=".PsiquiatricoActivity" android:exported="false" />
        <activity android:name=".RespiratorioActivity" android:exported="false" />
        <activity android:name=".InfecciosoActivity" android:exported="false" />
        <activity android:name=".GastrointestinalActivity" android:exported="false" />
        <activity android:name=".NeurologicoActivity" android:exported="false" />
        <activity android:name=".MusculoEsqueleticoActivity" android:exported="false" />
